{
  "name": "surprise-teams",
  "type": "module",
  "version": "0.0.1",
  "engineStrict": true,
  "engines": {
    "node": ">=22 <23"
  },
  "scripts": {
    "astro": "astro",
    "build": "npm run build:base",
    "build:base": "npm run check && astro build",
    "check": "astro check && npm run fmt:check && npm run lint",
    "deps": "ncu -i --format group",
    "dev": "rm -r .wrangler && wrangler types && concurrently \"astro dev\" \"npm run typewatch\"",
    "info": "astro info",
    "fmt": "prettier . --write",
    "fmt:check": "prettier . --check",
    "lint": "eslint .",
    "preview": "astro preview",
    "typewatch": "astro check --watch --preserveWatchOutput",
    "wip": "git commit -m \"[CF-Pages-Skip] WIP\" && git push"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.3.0",
    "@astrojs/react": "^4.2.1",
    "@astrojs/tailwind": "^5.1.5",
    "@fontsource-variable/sixtyfour": "^5.2.5",
    "@libsql/client": "^0.14.0",
    "@radix-ui/react-popover": "1.1.6",
    "@sentry/astro": "^8.54.0",
    "@sentry/cloudflare": "^8.54.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "astro": "^5.5.4",
    "dotenv": "^16.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.23.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@typescript-eslint/parser": "^8.27.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.14.0",
    "npm-check-updates": "^17.1.15",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "tsx": "^4.19.3",
    "typescript-eslint": "^8.27.0"
  }
}
