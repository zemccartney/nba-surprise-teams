{
  "name": "surprise-teams",
  "type": "module",
  "version": "0.0.1",
  "engineStrict": true,
  "engines": {
    "node": ">=22 <23"
  },
  "scripts": {
    "astro": "astro",
    "archive:all": "tsx archiver.ts",
    "archive:latest": "tsx archiver.ts --latest",
    "build": "npm run check && astro build",
    "check": "astro check && npm run fmt:check && npm run lint",
    "deps": "ncu -i --format group",
    "dev": "rm -r .wrangler || true && wrangler types && concurrently \"astro dev\" \"npm run typewatch\"",
    "info": "astro info",
    "fmt": "prettier . --write",
    "fmt:check": "prettier . --check",
    "lint": "eslint .",
    "preview": "astro preview",
    "typewatch": "astro check --watch --preserveWatchOutput",
    "wip": "git commit -m \"[CF-Pages-Skip] WIP\" && git push"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.5.1",
    "@astrojs/react": "^4.2.6",
    "@fontsource-variable/sixtyfour": "^5.2.5",
    "@fontsource/iosevka-curly": "^5.2.5",
    "@radix-ui/react-popover": "1.1.11",
    "@sentry/astro": "^9.15.0",
    "@sentry/cloudflare": "^9.15.0",
    "@tailwindcss/vite": "^4.1.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "astro": "^5.7.9",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.15.3",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.25.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@typescript-eslint/parser": "^8.31.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.12.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unicorn": "^59.0.0",
    "globals": "^16.0.0",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tsx": "^4.19.4",
    "typescript-eslint": "^8.31.1"
  }
}
