{
  "name": "surprise-teams",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "archive:all": "tsx archiver.ts",
    "archive:latest": "tsx archiver.ts --latest",
    "astro": "astro",
    "build": "npm run check && astro build",
    "cf:wip": "git commit -m \"[CF-Pages-Skip] WIP\" && git push",
    "check": "astro check && npm run fmt:check && npm run lint",
    "deps": "ncu -i --format group",
    "dev": "rm -r .wrangler || true && wrangler types && concurrently \"astro dev\" \"npm run typewatch\"",
    "fmt": "prettier . --write",
    "fmt:check": "prettier . --check",
    "info": "astro info",
    "lint": "eslint .",
    "preview": "astro preview",
    "typewatch": "astro check --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.5.3",
    "@astrojs/react": "^4.3.0",
    "@fontsource-variable/sixtyfour": "^5.2.5",
    "@fontsource/iosevka-curly": "^5.2.5",
    "@radix-ui/react-popover": "1.1.14",
    "@sentry/astro": "^9.22.0",
    "@sentry/cloudflare": "^9.22.0",
    "@tailwindcss/vite": "^4.1.7",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "astro": "^5.8.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.15.3",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.27.0",
    "@eslint/json": "^0.12.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@typescript-eslint/parser": "^8.33.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-package-json": "^0.31.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.2.0",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tsx": "^4.19.4",
    "typescript-eslint": "^8.33.0"
  },
  "engines": {
    "node": ">=22 <23"
  },
  "engineStrict": true
}
